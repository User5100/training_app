    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "cors": "^2.8.4",
    "devcert-with-localhost": "^0.3.3",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "graphql-yoga": "^1.13.1",
    "helmet": "^3.12.0",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^immutable@4.0.0-rc.9",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.4",
    "mongoose": "^5.0.7",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "ramda": "^0.25.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-apollo": "^2.1.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "sequelize": "^4.37.1",
    "start-server-webpack-plugin": "^2.2.1",
    "styled-components": "^3.0.2",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "web-push": "^3.3.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^2.0.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "start":
      "webpack-dev-server --https --key ./private/key.pem --cert ./private/cert.pem --open --config webpack.dev.js",
    "prod": "node dist-server/index.js",
    "start:api": "webpack --config ./api/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build:api": "webpack --config ./api/webpack.prod.js",
    "prepcerts":
      "node ./server/gen-certs.js && rm -rf dist/*.pem && mkdir -p dist && cp private/*.pem dist/",
    "prepvapid": "web-push generate-vapid-keys --json > ./private/vapid.json",
    "pre:test": "yarn db:migrate",
    "test": "jest",
    "test:report": "open coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "format":
      "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{ts, tsx}\"",
    "lint": "eslint **/*.{ts, tsx} --quiet",
    "sql:migration:create": "sequelize migration:create",
    "sql:migration:test": "NODE_ENV=test yarn sql:migration:create",
    "sql:migrate": "sequelize db:migrate",
    "sql:help": "sequelize --help",
    "debug:api": "node --inspect --inspect-brk ./api/index.ts"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "collectCoverageFrom": ["**/*.{ts, tsx}", "!**/node_modules/**"],
    "coverageThreshold": {
      "./src": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 100
      }
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": ["src", "api", "server", "node_modules"],
    "moduleFileExtensions": ["js", "jsx", "json", "ts", "tsx", "node"],
    "modulePaths": ["<rootDir>/src", "<rootDir>/api"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jest-environment-jsdom"
  },
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "@types/enzyme": "^3.1.9",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "^0.0.38",
    "@types/faker": "^4.1.2",
    "@types/graphql": "^0.12.4",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mongoose": "^5.0.5",
    "@types/morgan": "^1.7.35",
    "@types/pg": "^7.4.5",
    "@types/ramda": "^0.25.21",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.5",
    "@types/react-loadable": "^5.3.3",
    "@types/react-redux": "^5.0.15",
    "@types/redux": "^3.6.0",
    "@types/sequelize": "^4.27.10",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "jest": "^22.4.2",
    "jest-styled-components": "^5.0.1",
    "jsdom": "^11.6.1",
    "module-resolver": "^1.0.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "react-testing-library": "^3.1.3",
    "script-loader": "^0.7.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.5",
    "tsconfig-paths": "^3.3.2",
    "worker-loader": "^1.1.1"
  }
}
